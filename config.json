{
  "initialUrl": "https://web.snapchat.com",
  "autoInject": false,
  "siteMode": "universal",
  
  "replyRules": [
    {
      "match": "?",
      "reply": "I'll check and get back to you soon!",
      "priority": 1
    },
    {
      "match": "hello",
      "reply": "Hey! How's it going?",
      "priority": 2,
      "caseSensitive": false
    },
    {
      "match": "hi",
      "reply": "Hi there! ðŸ‘‹",
      "priority": 2,
      "caseSensitive": false
    },
    {
      "match": "thanks",
      "reply": "You're welcome!",
      "priority": 1,
      "caseSensitive": false
    },
    {
      "match": "bye",
      "reply": "Talk to you later!",
      "priority": 1,
      "caseSensitive": false
    }
  ],
  
  "typingDelayRangeMs": [50, 150],
  "preReplyDelayRangeMs": [2000, 6000],
  
  "maxRepliesPerMinute": 5,
  "maxRepliesPerHour": 30,
  "maxReplyLength": 500,
  
  "randomSkipProbability": 0.15,
  
  "activeHours": {
    "start": "09:00",
    "end": "22:00"
  }
}
